
.container {
    width: 100%;
    height: 100vh;
}
.swiper-tab {
    width: 100%;
    border-bottom: 1rpx solid #f0f0f0;
    text-align: center;
    height: 34px;
    line-height: 34px;
    display: flex;
    flex-flow: row;
    justify-content: space-between;
    overflow-x: scroll;
    padding: 0 10px;
    box-sizing: border-box;    
}
.swiper-tab-item {
    min-width: 25%;
    padding-left: 5px;
    padding-right: 5px;
    color:#434343;
    flex-shrink: 0;
    color: #929292;
}
.swiper-tab-item text {
    font-size: 13px;
}
.active {
    color:#F65959;
}
.goods-list {
    width: 100%;
    height: auto;
    padding: 10px;
    display: flex;
    justify-content: flex-start;
    align-content: flex-start;
    flex-wrap: wrap;
    box-sizing: border-box;
}